    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: CANdoISO",
            "type": "debugpy",
            "request": "launch",
            "program": "src/can_cando/CANdo.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Python: Build",
            "type": "debugpy",
            "request": "launch",
            "module": "build",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Python Debugger: current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
    ]
}